<table class="table">
<thead>
<tr>
<td>Email</td>
<td>Name</td>
<td>Phone Number</td>
<td>Role</td>
</tr>
</thead>
<tbody>
@foreach(var item in Model){
    <tr>
<td>@item.user.Email</td>
<td>@item.user.UserName</td>
<td>@item.user.PhoneNumber</td>
<td>
@foreach(var roleName  in ViewBag.allRoles){

    @if(item.userRoles.Contains(roleName.toString())){
        <a class="btn btn-primary" href="">@roleName</a>
    }
    else{
        <a class="btn btn-outline-primary" href="">@roleName</a>
    }
}
    </td>
</tr>
}
</tbody>
</table>